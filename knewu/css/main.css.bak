h1{
	font-family: "Century Schoolbook", cursive, sans-serif;
	color: #3300CC;
	font-size: 150%;	
	margin: 0;
	padding: 0 0 1em 0;
}

h2{
	font-family: "Century Schoolbook", cursive, sans-serif;
	color: #660066;
	font-size: 130%;
}

body{
	background-color:#FFFFFF;
}

p {
	font-family: "Century Schoolbook", cursive, sans-serif;
	font-size: 125%;
	color: #660099;
}

#footer{
	text-align: center;
	font-family: "Century Schoolbook", cursive, sans-serif;
	font-size: 80%;
	color:#6600FF;
	background-color: #9999FF;
	display: block;
}
#footer a:hover {background:#CC99FF;}	
#wrapper {
	position: relative;
	text-align: left;
	width: 85%;
	margin: 0 auto 0 auto;
	padding: 0;
}

#navigation
{
	text-align: right;
}
#navigation a:hover {background:#CC99FF;}	
	
#links{
font-family: "Century Schoolbook", cursive, sans-serif;
} 
#links a:hover {background:#CC99FF;}	
#links a:visited {color:#9966CC;}


