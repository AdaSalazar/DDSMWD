h1{
	font-family: "Century Schoolbook", cursive, sans-serif;
	color: #3300CC;
	font-size: 150%;	
	margin: 0;
	padding: 0 0 1em 0;
}

h2{
	font-family: "Century Schoolbook", cursive, sans-serif;
	color: #660066;
	font-size: 130%;
}

body{
	background-color:#FFFFFF;
	min-width: 600px;
	padding: 0px;
	margin: 0px;
}

p {
	font-family: "Century Schoolbook", cursive, sans-serif;
	font-size: 125%;
	color: #660099;
}

#footer{
	text-align: center;
	font-family: "Century Schoolbook", cursive, sans-serif;
	font-size: 80%;
	color:#6600FF;
	background-color: #9999FF;
	display: block;
}
#footer a:hover {background:#CC99FF;}	
#wrapper {
	position: relative;
	text-align: left;
	width: 85%;
	margin: 0 auto 0 auto;
	padding: 0;
	
}

#navigation
{
	text-align: right;
}
#navigation a:hover {background:#CC99FF;}	
	
#links{
font-family: "Century Schoolbook", cursive, sans-serif;
} 
#links a:hover {background:#CC99FF;}	
#links a:visited {color:#9966CC;}




#topBanner{
	width: 100%;
	display:block;
	clear:both;
	min-width:680;
}
#leftImg{
	width: 30%;
	display:inline;
	margin-left:0;
	float: right;
	/*background:pink;*/
}
#centerImg{
	width: 38%;
	display:inline;
	float: right;
	/*background:purple;*/
	padding-left:2%;
}
#rightImg{
	width: 30%;
	display:inline;
	float: right;
	/*background:blue;*/
}
.logo{

	height: 30%;
}
.brush{
	/*width: 50%;	width: 50%;*/
	height: 30%;

}


.login {
	font-family: "Century Schoolbook", cursive, sans-serif;	
	font-size: 120%;
	font-weight: bold;
	color: #d94692;
	text-align: right;
	clear:both;
}
.login :hover{
	border: solid;
	border-width: 2px;
	border-radius: 10px;
	border: blue;
}
.searchButton{
	border-radius: 10px;

	background-image: linear-gradient(bottom, rgb(217,71,159) 25%, rgb(242,164,206) 70%);
	background-image: -o-linear-gradient(bottom, rgb(217,71,159) 25%, rgb(242,164,206) 70%);
	background-image: -moz-linear-gradient(bottom, rgb(217,71,159) 25%, rgb(242,164,206) 70%);
	background-image: -webkit-linear-gradient(bottom, rgb(217,71,159) 25%, rgb(242,164,206) 70%);
	background-image: -ms-linear-gradient(bottom, rgb(217,71,159) 25%, rgb(242,164,206) 70%);

	background-image: -webkit-gradient(
		linear,
		left bottom,
		left top,
		color-stop(0.25, rgb(217,71,159)),
		color-stop(0.7, rgb(242,164,206))
	);



}
.searchBox{
	border-radius: 10px;
}
#tabs{
	border: outset;
	border-width: 2px;
	border-radius: 10px;
	/*background: #d94692;*/
	background-image: linear-gradient(bottom, rgb(143,24,88) 25%, rgb(217,70,146) 65%);
	background-image: -o-linear-gradient(bottom, rgb(143,24,88) 25%, rgb(217,70,146) 65%);
	background-image: -moz-linear-gradient(bottom, rgb(143,24,88) 25%, rgb(217,70,146) 65%);
	background-image: -webkit-linear-gradient(bottom, rgb(143,24,88) 25%, rgb(217,70,146) 65%);
	background-image: -ms-linear-gradient(bottom, rgb(143,24,88) 25%, rgb(217,70,146) 65%);
	background-image: -webkit-gradient(
		linear,
		left bottom,
		left top,
		color-stop(0.25, rgb(143,24,88)),
		color-stop(0.65, rgb(217,70,146))
	);
	
	font-family: "Century Schoolbook", cursive, sans-serif;
	color: #580644;	
	text-shadow: rgb(90,90,90) 2px 2px 3px;
	
	display: block;
	padding: .2em .2em .1em .2em;
	/**/min-width:680px;
	clear:both;
	
 }
#noSelected a:hover{
	border-radius: 5px;
	/*background: #d98db4;*/
	background-image: linear-gradient(bottom, rgb(217,70,146) 25%, rgb(143,24,88) 70%);
	background-image: -o-linear-gradient(bottom, rgb(217,70,146) 25%, rgb(143,24,88) 70%);
	background-image: -moz-linear-gradient(bottom, rgb(217,70,146) 25%, rgb(143,24,88) 70%);
	background-image: -webkit-linear-gradient(bottom, rgb(217,70,146) 25%, rgb(143,24,88) 70%);
	background-image: -ms-linear-gradient(bottom, rgb(217,70,146) 25%, rgb(143,24,88) 70%);
	background-image: -webkit-gradient(
		linear,
		left bottom,
		left top,
		color-stop(0.25, rgb(217,70,146)),
		color-stop(0.7, rgb(143,24,88))
	);
	
	position: relative;
	float: left;
	padding: .5em 0em .5em 0em;
	/*padding: .2em 0em .2em 0em;*/
}
#selected a{
	padding: .5em 0em .5em 0em;/**/
	/**/background-image: url('../Images/MenuStar.png');
	background-position: 90% top;
	background-repeat:no-repeat;
	position: relative;
	display:inline; 
	float: left;
	width: 15%;
	text-align:center;
	text-decoration: none;
	font-weight: bold;
	font-size:110%;
	color:black;
	border: 1.5px  dash #d98db4;
	border-radius: 5px;
	
	
}
#noSelected a{
	padding: .5em 0em .8em 0em;
	display:inline; 
	float: left;
	width: 17%;
	text-align:center;
	text-decoration: none;
	font-weight: bold;
	font-size:110%;
	color:black;
}

#sForm{
	padding:5px 10px 0px 0px;
	display:inline-table; 
	text-align:right;
	/*border:1.5px  solid pink;*/
}

#content {
	font-family: "Century Schoolbook", cursive, sans-serif;
	border: 2px solid;
	clear: both;
	padding: 1em;
	color:#3300CC;
	clear:both;
}
#content p {
	margin: 0;
	padding: 1em;
}


#bigText{
	font-family: "Century Schoolbook", cursive, sans-serif;
	color: #663399;
	font-size: 120%;
}

